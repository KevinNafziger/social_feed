<section style="margin-top: 50px"></section>
  
    <h2> Feed Automator</h2> 

    <div class="row-9 w-row">  
          <br>
           <h2> Facebook: </h2>
          <% if @fb_is_hash == true %> 
            <% @fbs.each do |fb| %>
               <div class="column-19 w-col w-col-9">
                  <li style="font-size:22px;" class="list-item-2">
                  <%= fb.to_s.gsub(/("|\[|\])/, "").gsub(/{/, "").gsub(/}/, "")   %>
                  </li>
                  <br>
                  <br>
               </div>
            <% end %>
          <% else %>
                 <div class="column-19 w-col w-col-9">
                  <li style="font-size:22px;" class="list-item-2">
                  <%= @fbs.to_s.gsub(/("|\[|\])/, "").gsub(/{/, "").gsub(/}/, "")  %>
                  </li>
                  <br>
                  <br>
               </div>


          <% end %>
          <h2>Twitter:</h2>
          <% if @tw_is_hash == true %>
          <% @twitters.each do |tweet| %>
             <div class="column-19 w-col w-col-9">
                <li style="font-size:22px;" class="list-item-2">
                <%= tweet.to_s.gsub(/("|\[|\])/, "").gsub(/{/, "").gsub(/}/, "")   %>
                </li>
                <br>
                <br>
             </div>
          <% end %>
          <% else %>
              <div class="column-19 w-col w-col-9">
                <li style="font-size:22px;" class="list-item-2">
                <%= @twitters.to_s.gsub(/("|\[|\])/, "").gsub(/{/, "").gsub(/}/, "")   %>
                </li>
                <br>
                <br>
             </div>

          <% end %>
    </div>

